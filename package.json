{
  "name": "project-catwalk",
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(svg|png|jpg|ttf|woff|woff2)$": "jest-svg-transformer",
      ".+\\.(css|styl|less|sass|scss)$": "jest-css-modules-transform"
    }
  },
  "version": "1.0.0",
  "description": "Front End Capstone",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server/index.js",
    "build": "webpack --mode production",
    "test": "jest --collectCoverage",
    "dev:react": "live-server ./client/dist & webpack --mode development -w",
    "dev": "live-server ./client/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quintessestial-Queues/project-catwalk.git"
  },
  "author": "Zad, Leslie, Andrew",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Quintessestial-Queues/project-catwalk/issues"
  },
  "homepage": "https://github.com/Quintessestial-Queues/project-catwalk#readme",
  "dependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@loadable/component": "^5.14.1",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "css-loader": "^5.2.1",
    "enzyme-adapter-react-16": "^1.15.6",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "jest-css-modules-transform": "^4.2.0",
    "jest-enzyme": "^7.1.2",
    "jest-svg-transformer": "^1.0.0",
    "moment": "^2.29.1",
    "nodemon": "^2.0.7",
    "react": "^16.0.2",
    "react-dom": "^16.0.2",
    "react-icons": "^4.2.0",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0"
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "file-loader": "^6.2.0",
    "live-server": "^1.2.1"
  }
}
